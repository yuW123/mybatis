<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.wenyu.mybatis.dao.StudentDao">
    <select id="loadStudentById" parameterType="int" resultType="com.wenyu.mybatis.bean.Student">
        SELECT * FROM student WHERE studentId=#{id}
    </select>
    <select id="queryStudent" parameterType="java.util.Map" resultType="com.wenyu.mybatis.bean.Student">
        SELECT * FROM student WHERE studentName=#{studentName}
    </select>
</mapper>